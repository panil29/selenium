
public class MiniumNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
int[][] ab= {{2,4,5,7},{8,8,4,7},{1,2,9,0}};
		int min=ab[0][0];
		             for(int i=0;i<4;i++)
		             {
		            	 for(int j=0;j<4;j++)
		            	 {
		            		 if(ab[i][j]<min)
		            		 {
		            			 min=ab[i][j];
		            		 }
		            	 }
		             }
		             System.out.println(min);
	}

}
