
public class MiniumNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int min=abc[0][0];
int[][] ab= {{2,4,5},{3,4,7},{1,2,9}};
		             for(int i=0;i<3;i++)
		             {
		            	 for(int j=0;j<3;j++)
		            	 {
		            		 if(ab[i][j]<min)
		            		 {
		            			 min=ab[i][j];
		            		 }
		            	 }
		             }
	}

}
